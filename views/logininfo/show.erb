<!DOCTYPE html>
<%#include "logininfo.h" %>
<% tfetch(LoginInfo, loginInfo); %>
<html>
<head>
  <meta charset="UTF-8">
  <title><%= controller()->name() + ": " + controller()->activeAction() %></title>
</head>
<body>
<p style="color: red"><%=$ error %></p>
<p style="color: green"><%=$ notice %></p>

<h1>Showing Login Info</h1>
<dt>ID</dt><dd><%= loginInfo.id() %></dd><br />
<dt>Created at</dt><dd><%= loginInfo.createdAt() %></dd><br />
<dt>Updated at</dt><dd><%= loginInfo.updatedAt() %></dd><br />
<dt>Login</dt><dd><%= loginInfo.login() %></dd><br />
<dt>Passwd</dt><dd><%= loginInfo.passwd() %></dd><br />
<dt>Salt</dt><dd><%= loginInfo.salt() %></dd><br />
<dt>User Type</dt><dd><%= loginInfo.userType() %></dd><br />

<%== linkTo("Edit", urla("save", loginInfo.id())) %> |
<%== linkTo("Back", urla("index")) %>

</body>
</html>
